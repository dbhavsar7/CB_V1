<div class="form-group">
    <label>Quiz Name:</label>
    <input type="text" name="quizName" class="form-control" placeholder="Topic" required>
</div>
<div class="form-group">
    <label>Closing Date:</label>
    <input type="date" name="dateFinished" class="form-control" placeholder="Date" value="2019-05-01" required>
</div>
<div class="form-group">
    <label>Time Limit (in minutes):</label>
    <input type="number" name="times" class="form-control" placeholder="e.g. 15" required>
</div>
<div class="form-group">
    <label>Passing Score:</label>
    <input type="number" name="score" class="form-control" placeholder="e.g. 10" required>
</div>

