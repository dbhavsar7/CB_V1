<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <h3 class="page-header"><a href="#col3viewquizzes" data-toggle="collapse">Quizzes</a></h3>
        <div id="col3viewquizzes" class="collapse">
            <div class="responsive">
                <table class="table table-hover table-striped">
                    <thead>
                        <tr>
                            <th>Quiz Name</th>
                            <th>Ending Date</th>
                            <th>Time Limit</th>
                            <th>Max Score</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% quiz.forEach(function (data) { %>
                            <tr>
                                <td><%= data.quizName %></td>
                                <td><%= data.dateFinished %></td>
                                <td><%= data.times %></td>
                                <td><%= data.score %></td>
                                <td>
                                    <div style="float:left">
                                        <form method="post" action="/app/<%= data.quizId %>" id="<%= data.quizId %>" style="float:right">
                                            <input type="submit" name="delete" value='Delete' onClick="return confirm('Are you sure you want to delete?')" />
                                            <input type="hidden" name="_method" value="DELETE" />
                                        </form>
                                    </div>
                                </td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
</div>
